{
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        },
        {
            "files": ["*.js", "*.jsx", "*.tsx", "*.ts", "*.json"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
