{
  "name": "interview_counter_filipe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest **/*.test.ts",
    "print-test": "vitest **/*.test.ts --reporter=json --outputFile=./public/test-results/test-output.json",
    "ui-test": "playwright test",
    "prepare": "husky",
    "lint": "eslint . --fix --no-warn-ignored",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@playwright/test": "^1.45.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.1.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.0.8",
    "vitest": "^1.2.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@axe-core/playwright": "^4.9.1"
  }
}
